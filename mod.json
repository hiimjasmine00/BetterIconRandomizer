{
    "geode": "4.6.1",
    "gd": {
        "android": "2.2074",
        "win": "2.2074",
        "mac": "2.2074",
        "ios": "2.2074"
    },
    "version": "v1.5.3",
    "id": "hiimjustin000.better_icon_randomizer",
    "name": "Better Icon Randomizer",
    "developer": "hiimjasmine00",
    "description": "A mod based on randomizing icons and colors.",
    "dependencies": {
        "geode.custom-keybinds": {
            "importance": "required",
            "version": ">=v1.10.0",
            "platforms": ["win", "android", "mac"]
        },
        "geode.node-ids": ">=v1.2.0",
        "hiimjustin000.icon_randomizer_api": ">=v2.2.0",
        "hiimjustin000.more_icons": {
            "importance": "suggested",
            "version": ">=v1.11.0"
        }
    },
    "resources": {
        "spritesheets": {
            "BIR_RandomSheet": [
                "resources/RandomSheet/*.png"
            ]
        },
        "sprites": [
            "resources/*.png"
        ]
    },
    "settings": {
        "randomize-button-color": {
            "name": "Randomize Button Color (OLD)",
            "description": "The color of the randomize button. Use \"Randomize Button Color\" instead.",
            "type": "int",
            "default": 0
        },
        "randomize-button-color-new": {
            "name": "Randomize Button Color",
            "description": "The color of the randomize button.",
            "type": "string",
            "default": "Random",
            "one-of": [
                "Random",
                "Green",
                "Pink",
                "Blue",
                "Cyan",
                "Gray",
                "Dark Purple",
                "Dark Aqua"
            ]
        }
    },
    "links": {
        "community": "https://discord.gg/QVKmbvBXA7",
        "source": "https://github.com/hiimjasmine00/BetterIconRandomizer",
        "homepage": "https://www.hiimjasmine00.com"
    },
    "tags": [
        "customization",
        "interface",
        "offline"
    ]
}
