{
    "geode": "4.0.0-alpha.1",
    "gd": {
        "android": "2.2074",
        "win": "2.2074",
        "mac": "2.2074"
    },
    "version": "v1.4.4-beta.1",
    "id": "hiimjustin000.better_icon_randomizer",
    "name": "Better Icon Randomizer",
    "developer": "hiimjustin000",
    "description": "A mod based on randomizing icons and colors.",
    "dependencies": [
        {
            "id": "geode.node-ids",
            "version": ">=v1.12.0",
            "importance": "required"
        },
        {
            "id": "hiimjustin000.icon_randomizer_api",
            "version": ">=v1.1.3-beta.1",
            "importance": "required"
        }
    ],
    "resources": {
        "spritesheets": {
            "BIR_RandomSheet": [
                "resources/RandomSheet/*.png"
            ]
        },
        "sprites": [
            "resources/*.png"
        ]
    },
    "settings": {
        "randomize-button-color": {
            "name": "Randomize Button Color (OLD)",
            "description": "The color of the randomize button. Use \"Randomize Button Color\" instead.",
            "type": "int",
            "default": 0
        },
        "randomize-button-color-new": {
            "name": "Randomize Button Color",
            "description": "The color of the randomize button.",
            "type": "string",
            "default": "Random",
            "one-of": ["Random", "Green", "Pink", "Blue", "Cyan", "Gray", "Dark Purple", "Dark Aqua"]
        }
    },
    "links": {
        "community": "https://discord.gg/QVKmbvBXA7",
        "source": "https://github.com/hiimjustin000/BetterIconRandomizer",
        "homepage": "https://www.hiimjustin000.com"
    },
    "tags": [
        "customization",
        "interface",
        "offline"
    ]
}
